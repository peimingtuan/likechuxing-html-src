}
.sdewItem .itmeDetailA{
    font-size: 28rpx;
    color: #555555;
    line-height: 40rpx;
    padding-bottom: 12rpx;
}
.sdewItem .itmeDetailB{
    font-size: 28rpx;
    color: #555555;
    line-height: 40rpx;
    padding-left: 34rpx;
    position: relative
}

.sdewItem .itmeDetailB .number{
    position: absolute;
    left: 0;
}

.explain{
    font-size: 24rpx;
    color: #999999;
