/* pages/user/licenseInfo.wxss */
.LicenseInfo {
  background-color: #fff;
  text-align: center;
}
div{
  display: block;
  font-size:16px;
  color:#220505;
  font-weight:300;
  line-height: 28px;
}
.footer {
  width: 100%;
  height: 44rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: auto 80%;
  position: absolute;
  bottom: 40rpx;
}
.strong{
  color:#b60b0b;
}
.LicenseInfo .info-box {
  padding: 120rpx 0 160rpx;
  font-size:18px;
  color: #111;
}
.LicenseInfo .info-box .icon-img {
  display: block;
  margin: 0 auto 30rpx;
  width: 120rpx;
  height: 120rpx;
}
.LicenseInfo .info-box .msg {
  font-size: 14px;
  color: #999;
}
.LicenseInfo .footer {
  position: absolute;
  width: 690rpx;
  left: 30rpx;
  bottom: 30rpx;
}
.LicenseInfo .footer .tel {
  font-size: 12px;
  color: #999;
  line-height: 60rpx;
}
.LicenseInfo .footer .btn {
  height: 92rpx;
  font-size: 16px;
}

