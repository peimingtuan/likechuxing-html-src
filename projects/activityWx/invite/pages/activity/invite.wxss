/* pages/activity/invite.wxss */

.bg {
  position: relative;
  text-align: center;
  min-height: 100vh;
  background-color: #161723;
}

.footer {
  width: 100%;
  height: 44rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: auto 100%;
  position: absolute;
  bottom: 40rpx;
}

.alert_btn_container .btn {
  border-radius: 40rpx !important;
}

.alert_btn_container .btn-primary {
  background-color: #f81131 !important;
  border-color: #f81131 !important;
}

.SharePage {
  position: relative;
  overflow: hidden;
}

.SharePage .box {
  width: 100%;
  min-height: 1080rpx;
  padding-top: 640rpx;
  box-sizing: border-box;
  background-color: #fff;
  border-radius: 0 0 40rpx 40rpx;
  margin-bottom: 160rpx;
}

.SharePage .box .btn {
  color: #fff;
}

.SharePage .box .desc {
  font-size: 16px;
  text-align: center;
}

.SharePage .box .desc.show {
  opacity: 1;
}
.step1 .opt-box{
  margin-top: 40rpx;
}
.SharePage .box .opt-box {
  text-align: center;
}

.SharePage .box .input, .SharePage .box .btn {
  width: 560rpx;
  height: 88rpx;
  outline: none;
  border-radius: 44rpx;
  box-sizing: border-box;
  border: solid 1px #111;
  padding: 0 44rpx;
  margin: 20rpx auto;
  font-size: 15px;
}
.SharePage .box .pos{
  position: relative;
}
.SharePage .box .get{
  background-color: #e8e9eb;
  color:#666;
  position: absolute;
  width: 8em;
  font-size: 13px;
  line-height: 84rpx;
  height: 88rpx;
  box-sizing: border-box;
  text-align: center;
  top:0;
  right:95rpx;
  border:solid 1px #111;
  border-radius: 0 44rpx 44rpx 0;
  z-index: 10;
}

.SharePage .box .btn {
  font-size: 17px;
  border-color: #f81131;
  text-align: center;
  background: #f81131 linear-gradient(to right, #f81131, #f91557);
}

.SharePage .box .btn:active {
  transition: transform 100ms;
  transform: scale(0.99);
}

.SharePage .header {
  position: absolute;
  top: 0;
  width: 870rpx;
  height: 560rpx;
  background: #f81131 linear-gradient(to right, #f81131, #f91557);
  border-radius: 0 0 50% 50%;
  left: -60rpx;
  box-shadow: 0 10px 20px 0 rgba(248, 17, 49, 0.3);
}

.step2 {
  margin: 20rpx 60rpx;
  font-size: 18px;

}

.strong {
  font-size: 26px;
  color: #f81131;
}

.SharePage .title {
  margin: 120rpx auto 60rpx;
  width: 520rpx;
  height: 260rpx;
  background: url("data:image/png;base64,") no-repeat center;
  background-size: 100% auto;
}
.SharePage .rules{
  margin:60rpx 120rpx;
  font-size: 12px;
  text-align: left;
  line-height: 36rpx;
  color:#999;
  padding-bottom: 60rpx;
}
.SharePage .rules .rule{
  margin:16rpx 0;
}
.SharePage .rules .index{
  border-radius: 50%;
  text-align: center;
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  background-color:#f81131; 
  line-height: 30rpx;
  margin-top:3rpx;
  margin-left: -36rpx;
  margin-right: 6rpx;
  color:#fff;
}
.SharePage .rules .title{
  text-align: center;
  font-size: 15px;
  color:#333;
  height: auto;
  margin:40rpx auto 20rpx;
}

.SharePage .desc.mini{
  margin-top:20px;
  font-size: 12px;
}

.SharePage .desc {
  color: #514b49;
  font-size: 15px;
  text-align: center;
}

.delay1 {
  animation-delay: 0.2s;
}
.delay2 {
  animation-delay: 0.4s;
}

.mask {
  position: fixed;
  font-size: 15px;
  top: 0;
  left: 0;
  box-sizing: border-box;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.44);
  color: #fff;
  padding-top: 200px;
  padding: 200px 80px 0;
  text-align: center;
}

.animated {
  -webkit-animation-duration: 1s;
  animation-duration: 1s;
  -webkit-animation-fill-mode: both;
  animation-fill-mode: both;
}

.animated.faster {
  -webkit-animation-duration: 0.4s;
  animation-duration: 0.4s;
}

.animated.fast {
  -webkit-animation-duration: 0.6s;
  animation-duration: 0.6s;
}

.animated.infinite {
  -webkit-animation-iteration-count: infinite;
  animation-iteration-count: infinite;
}

.animated.hinge {
  -webkit-animation-duration: 2s;
  animation-duration: 2s;
}

@-webkit-keyframes slideInDown {
  from {
    -webkit-transform: translate3d(0, -100%, 0);
    transform: translate3d(0, -100%, 0);
    visibility: visible;
  }

  to {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

@keyframes slideInDown {
  from {
    -webkit-transform: translate3d(0, -100%, 0);
    transform: translate3d(0, -100%, 0);
    visibility: visible;
  }

  to {
    -webkit-transform: translate3d(0, 0, 0);
    transform: translate3d(0, 0, 0);
  }
}

.slideInDown {
  -webkit-animation-name: slideInDown;
  animation-name: slideInDown;
}







